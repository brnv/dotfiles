partial default xkb_types "mouse" {
    virtual_modifiers LevelThree,Alt;

    type "FOUR_LEVEL+ALT" {
        modifiers = Shift+LevelThree+Alt;
        map[None] = Level1;
        map[Shift] = Level2;
        map[LevelThree] = Level3;
        map[Shift+LevelThree] = Level4;
        map[Alt+LevelThree] = Level5;
        level_name[Level1] = "Base";
        level_name[Level2] = "Shift";
        level_name[Level3] = "Mod Base";
        level_name[Level4] = "Shift Mod";
        level_name[Level5] = "Mod Alt";
    };
};
