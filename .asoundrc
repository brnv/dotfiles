!defaults.pcm.card USB
defaults.ctl.card USB

defaults.pcm {
	device 0
	subdevice -1

	ipc_key 5678293
	ipc_gid audio
	ipc_perm 0660

	rate 0

	dmix {
		format "unchanged"
		rate defaults.pcm.rate
		card defaults.pcm.card
		device defaults.pcm.device
	}

	dsnoop {
		card defaults.pcm.card
		device defaults.pcm.device
	}

	file_format "raw"
	file_truncate true
}

defaults.namehint.extended 0

pcm.!default {
    type plug
    slave { pcm "jack" }
}

ctl.mixer0 {
    type hw
    card 1
}

pcm.jack {
    type jack
    playback_ports {
        0 system:playback_1
        1 system:playback_2
    }
    capture_ports {
        0 system:capture_1
        1 system:capture_2
    }
}
