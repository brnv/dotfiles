!defaults.pcm.card USB
defaults.ctl.card USB

defaults.pcm {
	device 0
	subdevice -1

	ipc_key 5678293
	ipc_gid audio
	ipc_perm 0660

	rate 0

	dmix {
		format "unchanged"
		rate defaults.pcm.rate
		card defaults.pcm.card
		device defaults.pcm.device
	}

	dsnoop {
		card defaults.pcm.card
		device defaults.pcm.device
	}

	file_format "raw"
	file_truncate true
}

defaults.namehint.extended 0
