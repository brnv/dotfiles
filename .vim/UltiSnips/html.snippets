	$1
</h4>
endsnippet

snippet ul "<ul>"
<ul>
	<li>
		$1
	</li>
</ul>
endsnippet

snippet ol "<ol>"
<ol>
	<li>
		$1
	</li>
</ol>
endsnippet

snippet li "<li>"
<li>
	$1
</li>
endsnippet

snippet b "<b>"
<b>
${0:${VISUAL}}
</b>
endsnippet

snippet a "<a>"
<a href="$1">
	$0
</a>
endsnippet

snippet f "form"
<form action="$1" method="${2:POST}">
	$0
</form>
endsnippet

snippet i "input"
<input type="${1:text}" name="$2" id="$2" value="$3" />
$0
endsnippet

snippet is "input type submit"
<input type="submit" value="$1" />
$0
endsnippet

snippet d "<div>"
<div ${1:class="$2"} ${3:id="$4"}>
	$0
</div>
endsnippet

snippet s "<span>"
