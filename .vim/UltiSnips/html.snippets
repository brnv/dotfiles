</span>
endsnippet

snippet scr "script"
<script type="text/javascript">
	$0
</script>
endsnippet

snippet scrs "script"
<script type="text/javascript" src="$0">
</script>
endsnippet

snippet container "bootstrap container"
<div class="container-fluid">
	$0
</div>
endsnippet

snippet row "bootstrap row"
<div class="row">
	$0
</div>
endsnippet

snippet md1 "col-md-1"
<div class="col-md-1">
	$0
</div>
endsnippet

snippet md6 "col-md-6"
<div class="col-md-6">
	$0
</div>
endsnippet

snippet img "<img/>"
<img src="$0" />
endsnippet

snippet button "button"
<button>
	$0
</button>
endsnippet

snippet center "center"
<center>
${0:${VISUAL}}
</center>
endsnippet

snippet n "new line"
