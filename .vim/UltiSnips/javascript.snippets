snippet if "if" !b
if (${1}) {
	${0}
}
endsnippet

snippet l "console.log();" !b
console.log(${0});
endsnippet

snippet of "object function" !b
${1}: function(${2}) {
	${0}
},
endsnippet

snippet a "alert"
alert($1);
endsnippet

snippet ajax "jquery ajax"
$.ajax(${1:url}).
	done(function() {
		$2
	}).
	fail(function() {
		$3
	});
endsnippet

snippet r "return"
return $0;
endsnippet
